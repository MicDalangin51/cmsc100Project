{
  "name": "cmsc100project",
  "version": "0.0.1",
  "description": "blog app",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon src/index",
    "semistandard": "semistandard --fix",
    "tap": "c8 --lines 80 --functions 80 --branches 80 --statements 80 --check-coverage --per-file tap test/**/*.test.js --no-coverage",
    "test": "npm run semistandard && npm run tap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MicDalangin51/cmsc100Project.git"
  },
  "keywords": [
    "Node",
    "JS",
    "Web",
    "Server"
  ],
  "author": "Martin Ivan Dalangin",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MicDalangin51/cmsc100Project/issues"
  },
  "homepage": "https://github.com/MicDalangin51/cmsc100Project#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@fastify/view": "^7.3.0",
    "c8": "^7.12.0",
    "chance": "^1.1.9",
    "ejs": "^3.1.8",
    "eslint": "8.19.0",
    "eslint-config-semistandard": "16.0.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "4.1.0",
    "mongoose": "^6.8.2",
    "must": "^0.13.4",
    "nodemon": "^2.0.20",
    "semistandard": "^16.0.1",
    "tap": "^16.3.2"
  },
  "semistandard": {
    "plugins": [
      "html",
      "standard",
      "promise",
      "mocha",
      "chai-expect"
    ],
    "parser": "@babel/eslint-parser",
    "env": [
      "mocha"
    ],
    "globals": [
      "describe",
      "it",
      "after",
      "before",
      "expect"
    ]
  },
  "dependencies": {
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.6.0",
    "@fastify/swagger": "^7.6.1",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "fastify": "^4.11.0",
    "fastify-openapi-glue": "^4.1.1",
    "fastify-static": "^4.7.0",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jquery": "^3.6.3",
    "path": "^0.12.7",
    "uuid": "^9.0.0"
  }
}
